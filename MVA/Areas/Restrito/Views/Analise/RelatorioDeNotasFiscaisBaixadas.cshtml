@model IEnumerable<NOTAS.Models.Notas>

@{
    ViewBag.Title = "Relatório de Notas Fiscais";
}

<br />
<br />

<p>Quantidade de Notas Baixadas: @ViewBag.NotasBaixadas</p>
<p>Quantidade de Notas Canceladas: @ViewBag.Canceladas</p>
<p>Quantidade de Notas Não Baixadas: @ViewBag.NotasNaoEncontradas</p>

<br />

  @foreach (var item in Model)
  {
      <div>
          @item.Chave
          <hr />
      </div>
  }


<div id="btn-top-exportar" class="panel-heading">
    <a href="@Url.Action("DownloadDasNotas")" class="btn btn-primary">Baixar Notas</a>
</div>